#include <iostream>
#include <string>
#include "Stack_char.h"

using namespace std;

Stack::Stack():LinkedList()
{
}
Stack::~Stack()
{
}
void Stack::push(char value)
{
	insertAtFront(value);
		
}
char Stack::pop()
{
	char store=first->val;
	return store;
}
char& Stack::top()
{
	return *(first->val);
}
