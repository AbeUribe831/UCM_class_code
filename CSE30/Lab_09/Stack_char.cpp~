#include <iostream>
#include <string>
#include "Stack_char.h"

using namespace std;

Stack_char::Stack_char():LinkedList_char(){}
Stack_char::~Stack_char(){}
void Stack_char::push(char value){
	insertAtFront(value);
		
}
char Stack_char::pop(){
	char store=first->val;
	removeFromFront();
	return store;
}
char& Stack_char::top(){
	return first->val;
}
